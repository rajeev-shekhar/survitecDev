    <validationRules>
        <fullName>rp_PDDChangeNotAllowed</fullName>
        <active>true</active>
        <description>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(StageName, &quot;Preparing Proposal&quot;)), 
ISCHANGED(Potential_Deal_Duration_months__c), 
RecordType.DeveloperName = &quot;Deal&quot; 
)</errorConditionFormula>
        <errorDisplayField>Potential_Deal_Duration_months__c</errorDisplayField>
        <errorMessage>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_TTFCIChangeNotAllowed</fullName>
        <active>true</active>
        <description>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(StageName, &quot;Preparing Proposal&quot;)), 
ISCHANGED(Time_to_full_Contract_implementation__c), 
RecordType.DeveloperName = &quot;Deal&quot; 
)</errorConditionFormula>
        <errorDisplayField>Time_to_full_Contract_implementation__c</errorDisplayField>
        <errorMessage>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_billingMethodCRPRule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Create_Revenue_Projections__c == true,
ISBLANK(TEXT(Billing_Method__c)),
RecordType.DeveloperName =  &quot;Deal&quot;
)</errorConditionFormula>
        <errorDisplayField>Billing_Method__c</errorDisplayField>
        <errorMessage>You must select an Invoicing Interval when &quot;Create Revenue Projections&quot; is set to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_billingMethodChangeNotAllowed</fullName>
        <active>true</active>
        <description>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(StageName, &quot;Preparing Proposal&quot;)), 
ISCHANGED(Billing_Method__c), 
RecordType.DeveloperName = &quot;Deal&quot; 
)</errorConditionFormula>
        <errorDisplayField>Billing_Method__c</errorDisplayField>
        <errorMessage>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_billingMethodPDDMMandatory</fullName>
        <active>true</active>
        <description>You must enter a value when &quot;Create Revenue Projections&quot; is set to true.</description>
        <errorConditionFormula>AND( 
Create_Revenue_Projections__c == true, 
ISBLANK(TEXT(Potential_Deal_Duration_months__c)), 
RecordType.DeveloperName =  &quot;Deal&quot;
)</errorConditionFormula>
        <errorDisplayField>Potential_Deal_Duration_months__c</errorDisplayField>
        <errorMessage>You must enter a value when &quot;Create Revenue Projections&quot; is set to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_billingMethodTTFCIMandatory</fullName>
        <active>true</active>
        <description>You must enter a value when &quot;Create Revenue Projections&quot; is set to true.</description>
        <errorConditionFormula>AND( 
Create_Revenue_Projections__c == true, 
ISBLANK(Time_to_full_Contract_implementation__c),
RecordType.DeveloperName = &quot;Deal&quot;  
)</errorConditionFormula>
        <errorDisplayField>Time_to_full_Contract_implementation__c</errorDisplayField>
        <errorMessage>You must enter a value when &quot;Create Revenue Projections&quot; is set to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_billingMethodTTFCIRule</fullName>
        <active>true</active>
        <description>Value in &quot;Time to full contract implementation&quot; must be exactly divisible by &quot;Billing Method&quot;.</description>
        <errorConditionFormula>IF(Billing_Method_value__c &gt; 0, 
AND(
Create_Revenue_Projections__c == true,
MOD(Time_to_full_Contract_implementation__c, Billing_Method_value__c) &gt; 0,
RecordType.DeveloperName = &quot;Deal&quot; 
),
false)</errorConditionFormula>
        <errorMessage>Value in &quot;Time to full contract implementation&quot; must be exactly divisible by &quot;Invoicing Interval&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_billingMethodTTRDMandatory</fullName>
        <active>true</active>
        <description>You must enter a value when &quot;Create Revenue Projections&quot; is set to true.</description>
        <errorConditionFormula>AND( 
Create_Revenue_Projections__c == true, 
ISBLANK(Time_to_revenue_days__c),
OR( 
RecordType.DeveloperName = &quot;Deal&quot;, 
RecordType.DeveloperName = &quot;Spot&quot; 
)  
)</errorConditionFormula>
        <errorDisplayField>Time_to_revenue_days__c</errorDisplayField>
        <errorMessage>You must enter a value when &quot;Create Revenue Projections&quot; is set to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_createProjectionChangeStageNameRule</fullName>
        <active>true</active>
        <description>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(StageName, &quot;Preparing Proposal&quot;)), 
ISCHANGED(Create_Revenue_Projections__c), 
OR( 
RecordType.DeveloperName = &quot;Deal&quot;, 
RecordType.DeveloperName = &quot;Spot&quot; 
) 
)</errorConditionFormula>
        <errorDisplayField>Create_Revenue_Projections__c</errorDisplayField>
        <errorMessage>This field cannot be changed when the Stage is other than &quot;Preparing Proposal&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rp_createProjectionOnOppCreate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(StageName, &quot;Preparing Proposal&quot;)), 
Create_Revenue_Projections__c == true,
ISNEW(), 
OR( 
RecordType.DeveloperName = &quot;Deal&quot;, 
RecordType.DeveloperName = &quot;Spot&quot; 
) 
)</errorConditionFormula>
        <errorDisplayField>Create_Revenue_Projections__c</errorDisplayField>
        <errorMessage>This field cannot be set to true when the Stage is other than &quot;Preparing Proposal&quot;.</errorMessage>
    </validationRules>
</CustomObject>
